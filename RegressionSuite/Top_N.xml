    <parameter name="userName" value="qa_user1@eigenrisk.com" />
    <classes>
      <class name="com.eigenRisk.TestSuite.Top_N_List">
		<methods>
			<include name="topN_RemoveSingleMeasure" />
		</methods>
	  </class>
    </classes>
  </test>
  <test name="Top N - Remove Multiple Measures">
    <parameter name="userName" value="qa_user1@eigenrisk.com" />
    <classes>
